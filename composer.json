{
    "name": "oksweb/sms-api-php",
    "license": "MIT",
    "homepage": "https://oksweb.com",
    "type": "library",
    "description": "PHP library for the OKSWeb SMS service.",
    "keywords": [
        "SMS",
        "API",
        "OKS",
        "oksweb"
    ],
    "authors": [
        {
            "name": "OKSWeb and contributors",
            "homepage": "https://github.com/oksweb/sms-api-php/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "2.16.1"
    },
    "autoload": {
        "psr-4": {
            "OKSWeb\\SMS\\": "lib/"
        }
    }
}
